<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Foto</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
        <td>
            {{this.title}}
        </td>
        <td>
            ${{this.price}}
        </td>
        <td>
            <img src="{{this.thumbnail}}" alt="imagen de {{this.title}}">
        </td>
    </tr>
    {{/each}}
  </tbody>
</table>